<?xml version="1.0" encoding="utf-8"?>
<!-- Android 12 or higher -->
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="database" />
        <exclude domain="file" path="datastore/." />
    </cloud-backup>

    <device-transfer>
        <exclude domain="database" />
        <exclude domain="file" path="datastore/." />
    </device-transfer>
</data-extraction-rules>
